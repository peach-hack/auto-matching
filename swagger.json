{
  "swagger": "2.0",
  "info": {
    "title": "Auto Matching",
    "description": "API description in Markdown.",
    "version": "1.0.0"
  },
  "host": "localhost:5000",
  "schemes": [
    "http"
  ],
  "paths": {
    "/api/users/source-sites": {
      "get": {
        "summary": "サイト一覧",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "成功時のレスポンス",
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "integer",
                        "format": "int64"
                      },
                      "name": {
                        "type": "string",
                        "example": "ハッピーメール"
                      },
                      "url": {
                        "type": "string",
                        "example": "https://example.com"
                      },
                      "activate_flag": {
                        "type": "string",
                        "example": "有効"
                      },
                      "user_id": {
                        "type": "string",
                        "example": "tsu-nera@gmail.com"
                      },
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}