{
  "name": "auto-matching-nuxt",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/jest": "^23.3.10",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-typescript": "^1.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-import-resolver-nuxt": "^0.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-nuxt": "^0.2.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^23.6.0",
    "jest-vue-preprocessor": "^1.4.0",
    "minimist": "^1.2.0",
    "nuxt": "^2.3.4",
    "nuxt-property-decorator": "^1.3.1",
    "phantomjs-prebuilt": "^2.1.16",
    "ts-jest": "^23.10.5",
    "ts-node": "^3",
    "typescript-eslint-parser": "^21.0.2",
    "vue-class-component": "^6.3.2",
    "vue-eslint-parser": "^4.0.3",
    "vue-jest": "^3.0.2",
    "vue-property-decorator": "^7.2.0",
    "vuetype": "^0.3.2",
    "vuex-class": "^0.3.0"
  },
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "vuetype": "vuetype components",
    "vuetype:w": "vuetype -w components",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "eslint": "node_modules/.bin/eslint . --ext .vue,.js,.ts",
    "eslint:fix": "node_modules/.bin/eslint . --fix --ext .vue,.js,.ts"
  },
  "devDependencies": {
    "@types/node": "^10.12.10",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
